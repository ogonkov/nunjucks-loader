{
  "name": "simple-nunjucks-loader",
  "version": "1.0.0-alpha.8",
  "description": "Webpack loader for Nunjucks",
  "main": "lib/loader.js",
  "scripts": {
    "build": "rm -rf lib && babel src --out-dir=lib --copy-files",
    "lint": "eslint src",
    "pretest": "rm -rf coverage && npm run lint",
    "test": "jest",
    "posttest": "rm -rf ./test/bundles/",
    "preversion": "npm test",
    "postversion": "git push && git push --tags",
    "prepublishOnly": "npm run build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ogonkov/nunjucks-loader.git"
  },
  "keywords": [
    "webpack",
    "loader",
    "nunjucks",
    "template",
    "html-webpack-plugin"
  ],
  "author": "Aleksey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ogonkov/nunjucks-loader/issues"
  },
  "homepage": "https://github.com/ogonkov/nunjucks-loader#readme",
  "dependencies": {
    "loader-utils": "^1.2.3",
    "schema-utils": "^2.6.1"
  },
  "peerDependencies": {
    "nunjucks": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.7.4",
    "@babel/core": "^7.7.4",
    "@babel/preset-env": "^7.7.4",
    "coveralls": "^3.0.9",
    "eslint": "^6.7.1",
    "jest": "^24.9.0",
    "nunjucks": "^3.2.0",
    "uuid": "^3.3.3",
    "webpack": "^4.41.2",
    "webpack-cli": "^3.3.10"
  },
  "engines": {
    "node": ">= 8.9.0"
  }
}
