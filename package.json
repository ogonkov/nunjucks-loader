    "nunjucks",
    "template"
  ],
  "author": "Aleksey",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ogonkov/nunjucks-loader/issues"
  },
  "homepage": "https://github.com/ogonkov/nunjucks-loader#readme",
  "dependencies": {
    "loader-utils": "^1.2.3",
    "schema-utils": "^2.0.1"
  },
  "peerDependencies": {
    "nunjucks": "^3.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.0",
    "@babel/core": "^7.5.4",
    "@babel/preset-env": "^7.5.4",
    "eslint": "^6.0.1",
    "jest": "^24.8.0",
    "nunjucks": "^3.2.0",
    "uuid": "^3.3.2",
    "webpack": "^4.35.3"
